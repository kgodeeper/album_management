{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": ["off"],
        "no-tabs": ["off"],
        "eol-last": ["error"],
        "no-multiple-empty-lines": ["off"],
        "indent": ["off"]
    }
}
